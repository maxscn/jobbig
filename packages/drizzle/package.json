{
  "name": "@jobbig/drizzle",
  "module": "./dist/index.js",
  "type": "module",
  "version": "0.2.1",
  "zshy": {
    "exports": {
      ".": "./src/index.ts",
      "./postgres": "./src/postgres/index.ts",
      "./mysql": "./src/mysql/index.ts"
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0"
  },
  "peerDependencies": {
    "typescript": "^5",
    "drizzle-orm": ">=0.36.4"
  },
  "peerDependenciesMeta": {
    "drizzle-orm": {
      "optional": true
    }
  },
  "dependencies": {
    "@jobbig/core": "*",
    "mysql2": "^3.14.3",
    "postgres-js": "^0.1.0"
  },
  "scripts": {
    "build": "zshy"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./postgres": {
      "types": "./dist/postgres/index.d.cts",
      "import": "./dist/postgres/index.js",
      "require": "./dist/postgres/index.cjs"
    },
    "./mysql": {
      "types": "./dist/mysql/index.d.cts",
      "import": "./dist/mysql/index.js",
      "require": "./dist/mysql/index.cjs"
    }
  }
}
