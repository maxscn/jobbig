{
  "name": "@jobbig/core",
  "version": "0.1.0",
  "module": "./dist/index.js",
  "type": "module",
  "zshy": {
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "exports": {
      ".": "./src/index.ts",
      "./utils": "./src/utils/index.ts"
    }
  },
  "scripts": {
    "build": "zshy"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.cts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  }
}
