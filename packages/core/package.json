{
  "name": "@jobbig/core",
  "version": "0.1.27",
  "module": "./dist/index.js",
  "type": "module",
  "sideEffects": false,
  "zshy": {
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "exports": {
      ".": "./src/index.ts",
      "./utils": "./src/utils/index.ts",
      "./utils/*": "./src/utils/*",
      "./plugins": "./src/plugins/index.ts",
      "./plugins/*": "./src/plugins/*",
      "./sqs": "./src/sqs/index.ts"
    }
  },
  "scripts": {
    "build": "zshy",
    "build:lib": "zshy"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "ulid": "^3.0.1",
    "aws4fetch": "^1.0.20"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.cts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*",
      "import": "./dist/utils/*",
      "require": "./dist/utils/*"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.cts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*",
      "import": "./dist/plugins/*",
      "require": "./dist/plugins/*"
    },
    "./sqs": {
      "types": "./dist/sqs/index.d.cts",
      "import": "./dist/sqs/index.js",
      "require": "./dist/sqs/index.cjs"
    }
  }
}
